{
	"title": "Twitter",
	"$schema": "https://dotnet.github.io/docfx/schemas/v1.0/schema.json#",
	"version": "1.0.0",
	"description": "The schema for the Twitter timeline page",
	"id": "https://github.com/MicrosoftDocs/schemas/blob/master/schemas/Twitter.schema.json",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"metadata": {
			"properties": {
				"title": {
					"type": "string",
					"tags": [
						"localizable"
					]
				}
			},
			"type": "object"
		},
		"title": {
			"description": "The heading level 1 text.",
			"type": "string",
			"tags": [
				"localizable"
			]
		},
		"twitterAttribution": {
			"description": "The twitter handle (username) for the twitter user.",
			"type": "string"
		},
		"twitterTimeline": {
			"description": "The twitter-specific information to display the inline twitter timeline.",
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"href": {
					"type": "string",
					"description": "Link href to twitter timeline.",
					"contentType": "href"
				},
				"title": {
					"type": "string",
					"description": "Title for the twitter timeline, used as loading text."
				}
			}
		}
	},
	"metadata": "/metadata"
}